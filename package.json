{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "e2e-playwright": "playwright test e2e/inbox-detailed.spec.ts",
    "e2e-cucumber": "cucumber-js",
    "record:direct": "playwright codegen --target playwright-test --output e2e/inbox-record.spec.ts http://localhost:3000",
    "record:interactive": "playwright codegen http://localhost:3000",
    "record:viewport": "playwright codegen --target playwright-test --output e2e/inbox-record.spec.ts --viewport-size 1280,720 http://localhost:3000",
    "record:mobile": "playwright codegen --target playwright-test --output e2e/inbox-record.spec.ts --device=\"iPhone 13\" http://localhost:3000",
    "clean": "rm -rf .nuxt dist .output node_modules/.cache"
  },
  "dependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@iconify-json/lucide": "^1.2.66",
    "@iconify-json/simple-icons": "^1.2.50",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.11.13",
    "@nuxt/ui": "^4.0.0-alpha.1",
    "@unhead/vue": "^2.0.14",
    "@unovis/ts": "^1.6.0",
    "@unovis/vue": "^1.6.0",
    "@vueuse/nuxt": "^13.9.0",
    "cucumber": "^6.0.7",
    "date-fns": "^4.1.0",
    "ipx": "^2.1.1",
    "nuxt": "^4.0.3",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.6"
  },
  "resolutions": {
    "unimport": "4.1.1"
  }
}